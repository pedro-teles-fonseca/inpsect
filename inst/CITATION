
citHeader("To cite inspector in publications use:")

citEntry(entry = "Manual",
         title = "inspector: Validation of Arguments and Objects in User-Defined Functions",
         author = as.person("Pedro Fonseca"),
         year = "2020",
         note = "R package version 1.0.1",
         url = "https://pedro-teles-fonseca.github.io/inspector/",
         textVersion =
           paste("Pedro Fonseca (2020).",
                 "inspector: Validation of Arguments and Objects in User-Defined Functions.",
                 "R package version 1.0.1")
         )
